#!/home/users/furrert/work/oss/click-man/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'click-man==0.4.0','console_scripts','click-man'
__requires__ = 'click-man==0.4.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('click-man==0.4.0', 'console_scripts', 'click-man')()
    )
