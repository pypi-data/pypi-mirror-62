#!/home/savchenk/anaconda2/envs/py36/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pylogstash-context==0.1.0','console_scripts','logstash'
__requires__ = 'pylogstash-context==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pylogstash-context==0.1.0', 'console_scripts', 'logstash')()
    )
