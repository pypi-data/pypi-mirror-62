<?xml version="1.0"?>
<object name="portal_workflow">

  <object name="camcom_public_workflow" meta_type="Workflow" remove="True"/>
  <object name="camcom_folder_workflow" meta_type="Workflow" remove="True"/>
 
  <bindings>
    <default>
       <bound-workflow workflow_id="simple_publication_workflow"/>
    </default>

    <type type_id="Folder">
       <bound-workflow workflow_id="(Default)"/>
    </type>
    <type type_id="Topic">
       <bound-workflow workflow_id="(Default)"/>
    </type>

  </bindings>
</object>

