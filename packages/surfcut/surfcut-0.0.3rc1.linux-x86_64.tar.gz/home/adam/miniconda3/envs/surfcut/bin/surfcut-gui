#!/home/adam/miniconda3/envs/surfcut/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'surfcut==0.0.3rc1','console_scripts','surfcut-gui'
__requires__ = 'surfcut==0.0.3rc1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('surfcut==0.0.3rc1', 'console_scripts', 'surfcut-gui')()
    )
