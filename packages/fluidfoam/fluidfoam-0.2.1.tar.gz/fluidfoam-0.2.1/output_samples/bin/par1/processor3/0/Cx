FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      Cx;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    top
    {
        type            calculated;
        value           uniform 0;
    }
    bottom
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


