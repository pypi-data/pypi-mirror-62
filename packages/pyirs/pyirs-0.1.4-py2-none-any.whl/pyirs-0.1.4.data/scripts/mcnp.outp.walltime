#!python


import sys
from pirs.mcnp.outp import wall_time

non = '_'*7
for o in sys.argv[1:]:
    p, t, e = wall_time(o)
    if t: dTr = str(t-p)
    else: dTr = non
    if e: dTo = str(e-p)
    else: dTo = non
    if e and t: dEp = str(e-t)
    else:       dEp = non

    print '  '.join([o, dTr, dTo, dEp])

